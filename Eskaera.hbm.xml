<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="2taldea"
                   namespace="_2taldea">
	<class name="Eskaera" table="eskaera">
		<id name="Id" column="id" type="int">
			<generator class="native"/>
		</id>
		<property name="EskaeraZenb" column="eskaeraZenb" type="int" not-null="true"/>
		<property name="Izena" column="izena" type="string" length="45" not-null="true"/>
		<property name="Prezioa" column="prezioa" type="float" not-null="true"/>
		<property name="MesaId" column="mesa_id" type="int" not-null="true"/>
		<property name="Activo" column="activo" type="byte" not-null="true"/>
	</class>
</hibernate-mapping>